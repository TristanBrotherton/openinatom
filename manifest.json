{
   "description": "Extends the Developer Tools, adding an ability to open resources in atom with a right click in the sources panel of the developer tools.",
   "devtools_page": "devtools.html",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
   },
   "manifest_version": 2,
   "name": "Atom",
   "permissions": [ "http://*/", "https://*/", "storage" ],
   "options_page": "options.html",
   "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
    },
   "version": "1"
}
